<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectpresetlightroom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/splash_bg2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/splash_bg2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.DetailCollection"></activity>
        <activity android:name=".activities.Detail" />
        <activity android:name=".activities.HowItWorks" />
        <activity android:name=".activities.WelcomeActivity" />
        <activity android:name=".activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MainActivity" />
    </application>

</manifest>